@{
    ViewData["Title"] = "Productos";
}

<div class="container mt-4">
    <div class="row">
        <div class="col-8">
            <h2 class="text-primary">Lista de Productos</h2>
        </div>
    </div>
    <br />
    <br />
    <table id="tblProductosColaborador" class="table table-striped table-bordered" style="width: 100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>Imagen</th>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Cantidad</th>
                <th>Categoría</th>
            </tr>
        </thead>

    </table>
</div>

@section Scripts {
    <script src="~/js/ProductoColaborador.js"></script>
}